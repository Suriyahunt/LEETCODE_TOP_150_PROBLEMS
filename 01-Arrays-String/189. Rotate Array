class Solution {
    public void rotate(int[] nums, int k) {
            k= k%nums.length;
       nums=rot(nums,0,nums.length-1);
        nums=rot(nums,0,k-1);
        nums=rot(nums,k,nums.length-1);
    }
    public static int[] rot(int[] nums,int start, int end){
        while(start <=end){
            int temp = nums[start];
            nums[start]= nums[end];
            nums[end] = temp;
            start++;
            end--;
        }
        return nums;
    }
}
